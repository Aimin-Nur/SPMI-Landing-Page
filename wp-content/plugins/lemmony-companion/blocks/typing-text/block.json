{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "lemmony-companion/typing-text",
  "version": "1.0",
  "title": "Typing Text",
  "category": "media",
  "description": "Dynamic typewritter effect for your text by Lemmony",
  "attributes": {
    "typingTexts": {
      "type": "array",
      "default": [
        "typing text goes here",
        "and also here"
      ]
    },
    "prefix": {
      "type": "string",
      "default": "Prefix",
      "source": "text",
      "selector": ".lemmony-typing-prefix"
    },
    "suffix": {
      "type": "string",
      "default": "suffix",
      "source": "text",
      "selector": ".lemmony-typing-suffix"
    },
    "typingDecoration": {
      "type": "string",
      "default": ""
    },
    "textAlign": {
      "type": "string"
    }
  },
  "example": {},
  "supports": {
    "anchor": true,
    "align": false,
    "color": {
      "background": false
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "textdomain": "lemmony-companion",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
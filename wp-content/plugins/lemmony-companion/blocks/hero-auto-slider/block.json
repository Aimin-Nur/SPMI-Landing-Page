{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "lemmony-companion/hero-auto-slider",
  "version": "1.0",
  "title": "Hero Auto Slider",
  "category": "widgets",
  "description": "Image slider with auto play by Lemmony",
  "attributes": {
    "images": {
      "type": "array",
      "default": []
    },
    "speed": {
      "type": "string",
      "default": "normal"
    },
    "aspectRatio": {
      "type": "string",
      "default": "1:1"
    }
  },
  "supports": {
    "align": true,
    "html": false
  },
  "textdomain": "lemmony-companion",
  "editorScript": "file:./index.js"
}
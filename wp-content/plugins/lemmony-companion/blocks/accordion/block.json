{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "lemmony-companion/accordion",
  "version": "1.0",
  "title": "Accordion",
  "category": "widgets",
  "description": "Accordion block by Lemmony",
  "attributes": {
    "title": {
      "type": "string"
    },
    "collapsed": {
      "type": "integer"
    }
  },
  "supports": {
    "anchor": true,
    "color": {
      "background": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": false
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "textdomain": "accordion",
  "editorScript": "file:./index.js"
}